# CodeVerse – copy this file to .env and fill in your values
# DO NOT COMMIT .env TO VERSION CONTROL

# Backend API base URL (no trailing slash)
EXPO_PUBLIC_API_URL=https://your-backend.onrender.com

# Required for real AI. Get a key at https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OAuth Client IDs (from Google / GitHub – NOT the redirect URL)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
EXPO_PUBLIC_GITHUB_CLIENT_ID=your_github_client_id

# Optional: pin redirect so it matches Google Console exactly
EXPO_PUBLIC_GOOGLE_REDIRECT_URI=https://auth.expo.io/@your-username/your-app
